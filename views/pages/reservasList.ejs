<section id="listareserva"  class="py-3">
    <div class="container">
        <h1 class="l-heading"><span class="text-primary">Reser</span>vas</h1>
        <% for (let i of allreservas){ %>
            <div class="card" style="width: 18rem;">
                <div class="card-header">
                    <ul class="list-group1">
                        <h5 class="card-title">Datos de la reserva</h5>
                        <li class="list-group-item">Fecha de llegada:
                            <%= i.fechaIngreso %>
                        </li>
                        <li class="list-group-item">Fecha de salida:
                            <%= i.fechaSalida %>
                        </li>
                        <li class="list-group-item">Cantidad de pasajeros:
                            <%= i.cantidadPasajeros %>
                        </li>
                        <li class="list-group-item">Cantidad de habitaciones:
                            <%= i.cantidadHabitaciones %>
                        </li>
                        <li class="list-group-item">Metodo de pago:
                            <%= i.metodoPago %>
                        </li>
                        <h5 class="card-title">Huesped</h5>
                        <li class="list-group-item">Nombre:
                            <%= i.owner.nombre %>
                        </li>
                        <li class="list-group-item">Apellido:
                            <%= i.owner.apellido %>
                        </li>
                        <li class="list-group-item">DNI:
                            <%= i.owner.dni %>
                        </li>
                    </ul>
                </div>
            </div>
            <a class="buttom3" href="/cancelar">
                <button type="submit" class="btn">Cancelar</button>
            </a>
            <% } %>
    </div>
</section>











